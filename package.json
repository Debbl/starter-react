{
  "name": "starter-react",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@debbl/eslint-config": "^3.8.10",
    "@tailwindcss/postcss": "^4.0.7",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.20.1",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.2",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3",
    "vite": "^7.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
